<UserControl x:Class="Knowte.NotesModule.Views.Notes"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             xmlns:myControls="clr-namespace:Knowte.Common.Controls;assembly=Knowte.Common"
             xmlns:prism="http://www.codeplex.com/prism"
             xmlns:commonprism="clr-namespace:Knowte.Common.Prism;assembly=Knowte.Common"
             xmlns:prismMvvm="clr-namespace:Prism.Mvvm;assembly=Prism.Wpf"
             prismMvvm:ViewModelLocator.AutoWireViewModel="True">

    <myControls:TransitioningContentControl Foreground="{DynamicResource RG_BlackBrush}" FadeIn="True" FadeInTimeout="0.5" SlideIn="True" SlideInTimeout="0.25" SlideInFrom="-10" SlideInTo="20" Name="NotesRegion" prism:RegionManager.RegionName="{x:Static commonprism:RegionNames.NotesRegion}"/>
</UserControl>
